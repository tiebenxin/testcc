<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content_part"
    style="@style/Collection.Frame"
    android:layout_width="match_parent"
    android:layout_height="140dp"
    android:background="@drawable/ease_mm_listitem"
    android:baselineAligned="false"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="68dp"
        android:layout_height="77dp"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:background="#fff"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/starttime_bg"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="vertical">

            <TextView
                android:id="@+id/starttime"
                android:textStyle="bold"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingTop="6dp"
                android:paddingStart="2dp"
                android:paddingEnd="0dp"
                android:gravity="center_vertical"
                android:textColor="@color/font_color_313131"
                android:textSize="12sp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/date_bg"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="vertical">

            <TextView
                android:id="@+id/date"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColor="@color/font_color_313131"
                android:textSize="12sp"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="140dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="77dp"
            android:paddingTop="17dp"
            android:paddingBottom="17dp">

            <TextView
                android:id="@+id/call_title"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:ellipsize="end"
                android:maxEms="12"
                android:maxWidth="220dp"
                android:singleLine="true"
                android:text=""
                android:textColor="@color/font_color_313131"
                android:textSize="15sp"/>

            <TextView
                android:id="@+id/generator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true"
                android:ellipsize="end"
                android:maxEms="8"
                android:singleLine="true"
                android:text="@string/generator"
                android:textColor="#c1c1c1"
                android:textSize="13sp"/>
        </RelativeLayout>

        <TextView
            android:id="@+id/splitter"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="center"
            android:background="#f1f1f1"/>

        <LinearLayout
            android:id="@+id/most_right"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:gravity="start|center_vertical">

            <ImageView
                android:id="@+id/image0"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/image1"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/image2"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/image3"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/image4"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/image5"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/image6"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/icon_contact"
                android:textSize="12sp"/>

            <TextView
                android:id="@+id/count"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_marginStart="5dp"
                android:background="@drawable/checkbox_empty"
                android:gravity="center"
                android:textColor="#919191"
                android:textSize="13sp"
                android:visibility="gone"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>